<Layouts.app flash={@flash} current_scope={@current_scope} conn={@conn}>
  <.header>
    Edit Task category {@category.id}
    <:subtitle>Use this form to manage category records in your database.</:subtitle>
  </.header>

  <.category_form
    changeset={@changeset}
    action={~p"/categories/#{@category}"}
    return_to={~p"/categories"}
  />
</Layouts.app>
